{
  "venvPath": ".",
  "venv": ".venv",
  "include": [
    "."
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/.*",
    "frontend"
  ],
  "executionEnvironments": [
    {
      "root": ".",
      "pythonVersion": "3.14",
      "pythonPlatform": "Windows",
      "extraPaths": [
        "."
      ]
    }
  ],
  "typeCheckingMode": "basic",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "reportCallIssue": "none",
  "reportAttributeAccessIssue": "warning",
  "useLibraryCodeForTypes": true
}
